<div class="liker">
  <%= render @liker %>
</div>

<hr>
<h2>Matches</h2>
<div class="matches-list">
  <p><%= link_to "Look for new matches", look_for_new_matches_liker_path(@liker) %></p>

  <% @liker.matches.each do |match| %>
    <%= render match.tinder_user %>
  <% end %>
</div>

<hr>
<h2>People liked</h2>
<div class="likes-list">
  <%= render partial: "likes/list", locals: { liker: @liker } %>
</div>
